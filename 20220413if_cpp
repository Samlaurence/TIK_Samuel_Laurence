#include <iostream>
using namespace std;

int main() {
    /*
    jika nilai > 100 atau < tidak boleh
    jika nilai >=90 lulus sangat memuaskan
    jika nilai >=80 lulus memuaskan
    jika nilai >=75 cukup
    <= 75 tidak lulus
    
     */
int nilai;

cout << "masukan nilai = "; cin>> nilai;
if (nilai > 100 || nilai < 0){
    cout << " tidak boleh";
}
else if (nilai >= 90){
cout << " lulus sangat memuaskan";
}
else if (nilai >= 80){
    cout <<" lulus memuaskan";
}
else if (nilai >= 75){
    cout<< " lulus";
}
else {
     cout<< " tidak lulus"; 
}
}
