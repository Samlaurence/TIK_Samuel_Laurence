#include <iostream>

using namespace std;
int main(){
    string namaSiswa [4] = {"Anniko", "Hanis", "Samuel", "Justin"};
    cout<<namaSiswa[0]<<endl<<namaSiswa[1]<<endl<<namaSiswa[2]<<endl<<namaSiswa[3]<<endl;
    
    return 0;
}

#include <iostream>

using namespace std;
int main(){
    string namaSiswa [] = {"Anniko", "Hanis", "Samuel", "Justin"};
    for (int i = 0; i <4; i++ ){
    cout<<namaSiswa[i]<<endl;
    
    return 0;
}
}
