#include <iostream>

using namespace std;
int main(){
    int angka [10] = {123, 123 , 445,  6564, 757, 5675, 6575, 6575, 567};
    for (int i = 0; i<10; i++){
    cout<< angka [i] <<endl;
    
    }  

 
 return 0;
 }

#include <iostream>

using namespace std;
int main(){
    int angka [3][4] = {
        {1, 2, 4, 5},
        {3, 4, 5, 6},
        {5, 6, 4, 4},
        
        
    
    };
    for(int i = 0; i<3; i++){
        for(int j = 0; j<4; j++){
            cout<<angka[i][j];
        }
            cout<<endl;
    }
    
cout<<angka[0][0]<<endl;
cout<<angka[0][1]<<endl;
cout<<angka[0][2]<<endl;


 return 0;
        }
